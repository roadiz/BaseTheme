{
  "name": "BaseTheme",
  "description": "Blank Roadiz theme - This theme is meant to get a fresh start to create a theme.",
  "author": {
    "name": "Rezo Zero",
    "url": "https://www.rezo-zero.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roadiz/BaseTheme"
  },
  "bugs": {
    "url": "https://github.com/roadiz/BaseTheme/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "add-force": "git add --force static/css/* static/img/* static/js/* static/vendors/* Resources/views/svg/sprite.svg.twig Resources/views/partials/css-inject.html.twig Resources/views/partials/js-inject.html.twig  Resources/views/partials/js-inject-legacy.html.twig Resources/views/partials/js-inject-modern.html.twig",
    "dev": "concurrently \"rm -rf static\" \"stencil build --dev --watch\" \"better-npm-run dev\"",
    "build": "concurrently \"rm -rf static\" \"stencil build --docs --serviceWorker\" \"better-npm-run build\"",
    "dev-livereload": "concurrently \"rm -rf static\" \"stencil build --dev --watch\" \"better-npm-run dev-livereload\"",
    "generate": "stencil generate"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "Roadiz-front:*",
        "REFRESH_ON_CHANGE": false
      }
    },
    "dev-livereload": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "Roadiz-front:*",
        "REFRESH_ON_CHANGE": true
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "Roadiz-front:*"
      }
    }
  },
  "devDependencies": {
    "@babel/cli": "~7.5.0",
    "@babel/core": "~7.5.0",
    "@babel/plugin-proposal-class-properties": "~7.5.0",
    "@babel/plugin-proposal-decorators": "~7.4.4",
    "@babel/plugin-proposal-do-expressions": "~7.5.0",
    "@babel/plugin-proposal-export-default-from": "~7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "~7.5.2",
    "@babel/plugin-proposal-function-bind": "~7.2.0",
    "@babel/plugin-proposal-function-sent": "~7.5.0",
    "@babel/plugin-proposal-logical-assignment-operators": "~7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.4.4",
    "@babel/plugin-proposal-numeric-separator": "~7.2.0",
    "@babel/plugin-proposal-optional-chaining": "~7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "~7.5.0",
    "@babel/plugin-proposal-throw-expressions": "~7.2.0",
    "@babel/plugin-syntax-dynamic-import": "~7.2.0",
    "@babel/plugin-syntax-import-meta": "~7.2.0",
    "@babel/plugin-transform-runtime": "~7.5.0",
    "@babel/plugin-transform-typescript": "~7.5.2",
    "@babel/preset-env": "~7.5.3",
    "@babel/register": "~7.4.4",
    "@babel/runtime": "~7.5.3",
    "@dogstudio/highway": "^2.1.3",
    "@stencil/core": "1.7.4",
    "@stencil/sass": "^1.1.0",
    "babel-eslint": "~10.0.2",
    "babel-loader": "~8.0.6",
    "better-npm-run": "~0.1.1",
    "bootstrap": ">=4.3.1",
    "clean-webpack-plugin": "~3.0.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "~5.0.3",
    "css-loader": "~3.0.0",
    "css-mqpacker": "~7.0.0",
    "cssnano": "~4.1.10",
    "debug": "~4.1.1",
    "del": "~5.0.0",
    "es6-promise": "~4.2.8",
    "eslint": "~6.0.1",
    "eslint-config-standard": "~12.0.0",
    "eslint-loader": "~2.2.1",
