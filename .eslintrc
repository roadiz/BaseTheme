{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": false
    },
    "rules": {
        "no-console": 0,
        "no-undef": 0,
        "no-unused-vars": 0
    }
}
